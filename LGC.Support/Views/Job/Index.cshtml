<script>
    function ConfirmDelete(id) {
        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, delete it!'
        }).then((result) => {
            if (result.isConfirmed) {
                $.post('@Url.Action("Delete","Product")', { id: id }, function (data) {
                    if (data.result == "ok") {
                        Swal.fire({
                            //position: 'top-end',
                            icon: 'success',
                            title: 'Your job service has been deleted.',
                            showConfirmButton: false,
                            timer: 2000
                        })
                        setTimeout(function () {
                            location.reload();
                        }, 2000);
                    } else {
                        Swal.fire({
                            //position: 'top-end',
                            type: 'error',
                            title: data.result,
                            showConfirmButton: false,
                            timer: 2000
                        })
                    }
                });
            }
        })
    }
</script>